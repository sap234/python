class student:
    
    def __init__(self,n='.',m=0):
        self.name=n
        self.marks=m
    
    def display(self):
        print('hi',self.name)
        print('your marks',self.marks)
        
print("---------------------------")
s1=student('lakshmi rai',880)
s1.display()
print("---------------------------")
